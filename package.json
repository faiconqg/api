{
  "name": "@faicon/api",
  "version": "0.0.3",
  "description": "API helper for react. https://github.com/faiconqg/api",
  "main": "lib/api.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
